/* pages/search/search.wxss */
.m-searchBar {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.u-input {
  background: #f1f1f1;
  line-height: 70rpx;
  height: 70rpx;
  display: block;
  font-size: 28rpx;
  padding-left: 80rpx;
  border-radius: 16rpx;
  color:#888;
  position: relative;
}
.u-input::before {
  content: '';
  position: absolute;
  left: 26rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 34rpx;
  height: 34rpx;
  background-size: 34rpx 34rpx;
  background-position: center center;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}
.u-sub-title {
  padding: 20rpx;
  font-size: 42rpx;
  font-weight: bold;
}